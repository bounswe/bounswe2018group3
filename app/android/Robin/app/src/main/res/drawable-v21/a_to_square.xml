<animated-vector
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:aapt="http://schemas.android.com/aapt">
    <aapt:attr name="android:drawable">
        <vector
            android:name="vector"
            android:width="24dp"
            android:height="24dp"
            android:viewportWidth="24"
            android:viewportHeight="24">
            <path
                android:name="line2"
                android:pathData="M 12 2 L 22 22"/>
            <path
                android:name="line3"
                android:pathData="M 6 12 L 18 12"/>
            <path
                android:name="path"
                android:pathData="M 6 12 L 18 12"/>
            <path
                android:name="line1"
                android:pathData="M 12 2 L 2 22"/>
        </vector>
    </aapt:attr>
    <target android:name="line3">
        <aapt:attr name="android:animation">
            <objectAnimator
                android:propertyName="pathData"
                android:duration="3000"
                android:valueFrom="M 6 12 L 18 12"
                android:valueTo="M 2 22 L 22 22"
                android:valueType="pathType"
                android:interpolator="@android:interpolator/fast_out_slow_in"/>
        </aapt:attr>
    </target>
    <target android:name="path">
        <aapt:attr name="android:animation">
            <objectAnimator
                android:propertyName="pathData"
                android:duration="3000"
                android:valueFrom="M 6 12 L 18 12"
                android:valueTo="M 2 2 L 22 2"
                android:valueType="pathType"
                android:interpolator="@android:interpolator/fast_out_slow_in"/>
        </aapt:attr>
    </target>
    <target android:name="line1">
        <aapt:attr name="android:animation">
            <objectAnimator
                android:propertyName="pathData"
                android:duration="3000"
                android:valueFrom="M 12 2 L 2 22"
                android:valueTo="M 2 2 L 2 22"
                android:valueType="pathType"
                android:interpolator="@android:interpolator/fast_out_slow_in"/>
        </aapt:attr>
    </target>
    <target android:name="line2">
        <aapt:attr name="android:animation">
            <objectAnimator
                android:propertyName="pathData"
                android:duration="3000"
                android:valueFrom="M 12 2 L 22 22"
                android:valueTo="M 22 2 L 22 22"
                android:valueType="pathType"
                android:interpolator="@android:interpolator/fast_out_slow_in"/>
        </aapt:attr>
    </target>
</animated-vector>
